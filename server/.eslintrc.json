{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "eslint-config-airbnb-base"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": 2021, // Update to the appropriate ECMAScript version you are targeting
        "sourceType": "module"
    },
    "plugins": [],
    "rules": {
        "react/jsx-filename-extension": [0], // This rule is specific to React, you may remove it if not using React
        "import/extensions": "off" // Disabling import/extensions rule
    }
}
